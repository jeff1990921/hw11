      "source": [
        "輸入年份，判斷是否閏年"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "tQzkOO-Lr-z7",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "def isLeapYear(year):\n",
        "  if year % 4:\n",
        "    return f'{year} is not leap year'\n",
        "  if not year % 100 and year % 1000:\n",
        "    return f'{year} is not leap year'\n",
        "  return f'{year} is leap year'"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "AxXVFCAGsT0D",
        "colab_type": "code",
        "outputId": "73a1f9a9-ba6f-4790-c074-9141a61c587a",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 187
        }
      },
      "source": [
        "for i in range(5):\n",
        "  year = int(input('input year:'))\n",
        "  print(isLeapYear(year))"
      ],
      "execution_count": 0,
